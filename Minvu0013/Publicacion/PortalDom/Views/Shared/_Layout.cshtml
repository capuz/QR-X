<html class="no-js">
<!--<![endif]-->
<head>

    <!-- Etiquetas metas -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    <title>DOM EN LÍNEA</title>

    <!-- favicon -->
    <link rel="icon" type="image/png" href="http://cdn.minvu.cl/NGM5.0/images/favicon.ico" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="http://cdn.minvu.cl/NGM5.0/js/html5/3.7.1/html5shiv.js"></script>
    <script src="http://cdn.minvu.cl/NGM5.0/js/respond.min.js"></script>
    <script src="http://cdn.minvu.cl/NGM5.0/js/css3-mediaqueries.js"></script>
    <![endif]-->

    <link href="http://cdn.minvu.cl/NGM5.0/css/bootstrap/3.3.7/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="http://cdn.minvu.cl/NGM5.0/css/style.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/MvcGrid/mvc-grid.css" rel="stylesheet" />
    <link href="~/Content/StyleOverride.css" rel="stylesheet" />

</head>
<body class="home cat-online">

    <!-- /////////////////////////
    ********** HEADER ************
    ///////////////////////// -->
    <!-- menu principal -->
    <nav class="navbar nav-minvu navbar-default header-site" role="navigation">
        <div class="container">
            <div class="navbar-left">
                 @{Html.RenderAction("Logo", "Home");}
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>

            <!-- nav links, formularios, y otros contenidos para el toggle ( palanca)  -->
            <div class="right-ondesktop  collapse navbar-collapse" id="navbar-collapse-1">
                <ul class="nav navbar-nav">
                    @{Html.RenderAction("Menu", "Home");}
                </ul>
            </div><!-- /.navbar-collapse -->
        </div>
    </nav>
    <!-- /////////////////////////
    ********** CONTENIDO ********
    ///////////////////////// -->

    <div class="content-site container clearfix">
        @Html.MvcSiteMap().SiteMapPath("BootstrapSiteMapPathHelperModel")
        @RenderBody()
        <br />
        <br />
    </div>
    @{ Html.RenderPartial("~/Views/Shared/_Footer.cshtml"); }

    <script src="http://cdn.minvu.cl/NGM5.0/js/jquery/1.10.2/jquery.min.js" type="text/javascript"></script>
    <script src="http://cdn.minvu.cl/NGM5.0/js/bootstrap/3.3.7/bootstrap.js" type="text/javascript"></script>
    <script src="http://cdn.minvu.cl/NGM5.0/js/modernizr.js" type="text/javascript"></script>
    <link href="http://cdn.minvu.cl/NGM5.0/toolbar-accesible/css/a11y.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.minvu.cl/NGM5.0/toolbar-accesible/css/a11y-toolbar.css" rel="stylesheet" type="text/css" />
    <link href="http://cdn.minvu.cl/NGM5.0/toolbar-accesible/css/a11y-fontsize.css" rel="stylesheet" type="text/css" />
    <script src="http://cdn.minvu.cl/NGM5.0/toolbar-accesible/js/a11y.js" type="text/javascript"></script>
    <script type="text/javascript"> var a11y_stylesheet_path = "http://cdn.minvu.cl/NGM5.0/toolbar-accesible/css/a11y-contrast.css"; </script>
    <script src="~/Scripts/MvcGrid/mvc-grid.min.js" type="text/javascript"></script>
    <script src="~/Scripts/MvcGrid/mvc-grid-lang-es.js"></script>
    <!-- toolbar accesible -->
    <div class="a11y-toolbar ltr">
        <ul>
            <li>
                <a href="#" role="button" class="a11y-toggle-contrast toggle-contrast" id="is_normal_contrast" title="Contraste">
                    <span class="offscreen">Toggle High Contrast</span>
                    <span class="aticon aticon-adjust" aria-hidden="true"></span>
                </a>
            </li>
            <li>
                <a href="#" role="button" class="a11y-toggle-fontsize toggle-fontsize" id="is_normal_fontsize" title="Fuente">
                    <span class="offscreen">Toggle Font size</span>
                    <span class="aticon aticon-font" aria-hidden="true"></span>
                </a>
            </li>
        </ul>
    </div>
    <script>
        $('.mvc-grid').mvcgrid();
    </script>
</body>
</html>