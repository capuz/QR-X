<!DOCTYPE html>
<html>
<head>

    <meta content="text/html; charset=utf-8" http-equiv="content-type">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    <title>Sistema Oficina DOM</title>
    <!-- favicon -->
    <link rel="icon" type="image/png" href="http://cdn.minvu.cl/NGM5.0/images/favicon.ico">
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="http://cdn.minvu.cl/NGM5.0/js/html5/3.7.1/html5shiv.js"></script><script src="http://cdn.minvu.cl/NGM5.0/js/respond.min.js"></script><script src="http://cdn.minvu.cl/NGM5.0/js/css3-mediaqueries.js"></script><![endif]-->
    <link href="http://cdn.minvu.cl/NGM5.0/css/bootstrap/3.3.7/bootstrap.css" rel="stylesheet"
          type="text/css">
    <link href="http://cdn.minvu.cl/NGM5.0/css/style.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Gridmvc.css" rel="stylesheet" />
</head>
<body>

    <!-- /////////////////////////
    ********** HEADER ************///////////////////////// -->
    <!-- menu principal -->

    <nav class="navbar nav-minvu navbar-default header-site" role="navigation">
        <div class="container">
            <div class="row" style="margin-bottom:8px;margin-top:8px;">
                <div class="col-xs-5 col-sm-4 col-md-4">
                    <img style="width: 167px; height: 98px;" title="Dom en línea" alt="Dom en línea" src="~/Images/logo-DOM.png" class="img-responsive">
                </div>
                <div class="col-xs-7 col-sm-8 col-md-8 text-right">
                    <h6>Juan Carlos Bodoque<br />MINVU<br />Administrador Nacional | Adm. Noticias</h6>
                </div>
            </div>


        </div>
    </nav>



    <div class="content-site  container clearfix" style="margin-top:20px;">
        <div class="row">
            <div class="col-md-3">
                <div class="panel panel-default">
                    <div class="bs-docs-sidebar">
                        <ul class="nav nav-list bs-sidenav  bs-docs-sidenav affix-top">
                            <li>
                                <a href="@Url.Action("Index","Menu")" sl-processed="1">
                                    <span class="glyphicon glyphicon-chevron-right"></span>
                                    Gestionar Menú
                                </a>

                            </li>
                            <li>
                                <a href="@Url.Action("Index","Organismo")" sl-processed="1">
                                    <span class="glyphicon glyphicon-chevron-right"></span>
                                    Gestionar Organismos
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index","Seccion")" sl-processed="1">
                                    <span class="glyphicon glyphicon-chevron-right"></span>
                                    Gestionar Secciones
                                </a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index","Noticia")" sl-processed="1">
                                    <span class="glyphicon glyphicon-chevron-right"></span>
                                    Gestionar Noticias
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- termina el sidebar  -->
            <!-- //////////////////////////////////
            ********** CONTENIDO CENTRAL ********/////////////////////////////////// -->
            <div id="main" class="col-md-9 clearfix" role="main">

                @RenderBody()
            </div>
        </div>
    </div>

    <!-- toolbar accesible -->

    <div class="a11y-toolbar ltr">
        <ul>
            <li>
                <a href="#" role="button" class="a11y-toggle-contrast toggle-contrast"
                   id="is_normal_contrast" title="Contraste">
                    <span class="offscreen">
                        Toggle
                        High Contrast
                    </span> <span class="aticon aticon-adjust" aria-hidden="true"></span>
                </a>
            </li>
            <li>
                <a href="#" role="button" class="a11y-toggle-fontsize toggle-fontsize"
                   id="is_normal_fontsize" title="Fuente">
                    <span class="offscreen">
                        Toggle
                        Font size
                    </span> <span class="aticon aticon-font" aria-hidden="true"></span>
                </a>
            </li>
        </ul>
    </div>
    <script src="http://cdn.minvu.cl/NGM5.0/js/jquery/1.10.2/jquery.min.js" type="text/javascript"></script>
    <script src="http://cdn.minvu.cl/NGM5.0/js/bootstrap/3.3.7/bootstrap.js" type="text/javascript"></script>
    <script src="http://cdn.minvu.cl/NGM5.0/js/modernizr.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.form.js"></script>
    <!-- Toolbar accesible -->

    <link href="http://cdn.minvu.cl/NGM5.0/toolbar-accesible/css/a11y.css" rel="stylesheet"
          type="text/css">
    <link href="http://cdn.minvu.cl/NGM5.0/toolbar-accesible/css/a11y-toolbar.css"
          rel="stylesheet" type="text/css">
    <link href="http://cdn.minvu.cl/NGM5.0/toolbar-accesible/css/a11y-fontsize.css"
          rel="stylesheet" type="text/css">
    <script src="http://cdn.minvu.cl/NGM5.0/toolbar-accesible/js/a11y.js" type="text/javascript"></script>
    <script type="text/javascript"> var a11y_stylesheet_path = "http://cdn.minvu.cl/NGM5.0/toolbar-accesible/css/a11y-contrast.css"; </script>

    <link href="~/Content/sweetalert2.min.css" rel="stylesheet" />

    <link href="~/Content/StyleOverride.css" rel="stylesheet" />


    <script src="~/Scripts/gridmvc.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    @*<script src="~/Scripts/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>*@
    <script src="~/Scripts/framework-1.0.0.js" type="text/javascript"></script>
    <script src="~/Scripts/tinymce.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap-filestyle.js" type="text/javascript"></script>
    <script src="~/Scripts/sweetalert2.min.js" type="text/javascript"></script>
    <script src="~/Scripts/pages/page.js" type="text/javascript"></script>
    <script type="text/javascript">
        var BASE_PATH = '@Url.Content("~")';
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
